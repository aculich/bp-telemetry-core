#!/usr/bin/env python3
"""
Cursor afterFileEdit hook.
Called after file is edited.
"""

import sys
from pathlib import Path

# Add blueplane to path
sys.path.insert(0, str(Path(__file__).parent.parent.parent / "src"))

from blueplane.capture.cursor_hooks import after_file_edit

if __name__ == "__main__":
    after_file_edit()

