#!/usr/bin/env python3
"""
Cursor afterAgentResponse hook.
Called after AI agent responds.
"""

import sys
from pathlib import Path

# Add blueplane to path
sys.path.insert(0, str(Path(__file__).parent.parent.parent / "src"))

from blueplane.capture.cursor_hooks import after_agent_response

if __name__ == "__main__":
    after_agent_response()

